  // BEGIN SECTION :: map

  section.map.closed
    a.map__link.button.button--red(href="#" js-map-button)
      span.open Скрыть 
      span.hide Показать всю 
      |  карту
    .map__container(style="background-image: url(img/map-bg.png)")
